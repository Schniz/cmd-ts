{
  "name": "cmd-ts",
  "version": "0.6.0",
  "homepage": "https://cmd-ts.now.sh",
  "license": "MIT",
  "author": "Gal Schlezinger",
  "main": "dist/cjs/index.js",
  "typings": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && tsc --project ./tsconfig.esm.json",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "// now-build": "typedoc --out public",
    "now-build": "mdbook build --dest-dir=public",
    "run-example": "yarn --silent ts-node src/example/app.ts",
    "start": "yarn build --watch",
    "prepublishOnly": "yarn build && yarn test",
    "test": "jest",
    "ts-node": "TS_NODE_TRANSPILE_ONLY=true node -r esm -r ts-node/register"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "sideEffects": false,
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "strip-ansi": "^6.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.1.0",
    "@types/node-fetch": "^2.5.5",
    "@types/request": "^2.48.4",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "cargo-mdbook": "^0.3.5",
    "docs-ts": "^0.3.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "esm": "^3.2.25",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "husky": "^4.2.1",
    "infer-types": "^0.0.2",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "request": "^2.88.0",
    "tempy": "^0.3.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typedoc": "^0.16.10",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {}
}
